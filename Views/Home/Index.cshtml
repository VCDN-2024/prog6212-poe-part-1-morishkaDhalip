@{
    ViewData["Title"] = "Home Page";
}

<style>
    body {
        background-color: #f4f4f4;
        color: #333;
    }

    .container {
        background-color: #fff;
        border-radius: 8px;
        box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
        padding: 20px;
        margin-top: 20px;
    }

    .jumbotron {
        background-color: #003366; /* Dark blue background */
        color: #ffffff;
        padding: 20px;
        border-radius: 8px;
    }

    .img-fluid {
        max-height: 150px;
        margin-bottom: 1rem;
    }

    .row {
        margin-top: 2rem;
    }

    .btn {
        width: 200px;
        background-color: #0056b3; /* Slightly lighter blue */
        border: none;
        color: white;
    }

        .btn:hover {
            background-color: #004080;
            color: white;
        }

    .info-section {
        background-color: #e7f0fa;
        padding: 15px;
        border-radius: 8px;
        margin-top: 20px;
    }
</style>

<div class="container">
    <div class="jumbotron text-center">
        <h1>Welcome to the Contract Monthly Claim System</h1>
        <p>This system simplifies the process of submitting and approving monthly claims for lecturers.</p>
    </div>

    <div class="row text-center">
        <div class="col-md-4">
            <img src="data:image/png;base64," alt="Submit Claim" class="img-fluid" />
            <h2>Submit a Claim</h2>
            <p>Click below to start a new claim submission.</p>
            <p><a class="btn btn-primary" asp-controller="Claims" asp-action="Create">Submit Claim &raquo;</a></p>
        </div>
        <div class="col-md-4">
            <img src="data:image/png;base64," alt="View Claim" class="img-fluid" />

            <h2>View Claims</h2>
            <p>Review and manage all submitted claims.</p>
            <p><a class="btn btn-secondary" asp-controller="Claims" asp-action="Index">View Claims &raquo;</a></p>
        </div>
        <div class="col-md-4">
            <img src="data:image/png;base64," alt="Upload Documents" class="img-fluid" />
            <h2>Upload Documents</h2>
            <p>Upload supporting documents for your claims.</p>
            <p><a class="btn btn-success" asp-controller="Claims" asp-action="Upload">Upload Documents &raquo;</a></p>
        </div>
    </div>

    <div class="info-section">
        <h3>Quick Tips</h3>
        <p>Get started quickly with these easy steps:</p>
        <ul>
            <li><strong>Submit a Claim:</strong> Click on 'Submit Claim' to enter your claim details and submit them for review.</li>
            <li><strong>View Claims:</strong> Check the status of your claims and see all past submissions.</li>
            <li><strong>Upload Documents:</strong> Attach any necessary documents to support your claim submissions.</li>
        </ul>
    </div>
</div>

